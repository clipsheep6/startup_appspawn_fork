{
    "name": "@ohos/startup_init_lite",
    "description": "ohos init lite process",
    "homePage": "https://gitee.com/openharmony",
    "version": "3.1",
    "license": "Apache License 2.0",
    "repository": "https://gitee.com/openharmony/startup_init_lite",
    "publishAs": "code-segment",
    "segment": {
        "destPath": "base/startup/init_lite"
    },
    "dirs": {},
    "scripts": {},
    "component": {
        "name": "init_lite",
        "subsystem": "startup",
        "adapted_system_type": [
            "small"
        ],
        "rom": "31KB",
        "ram": "~200KB",
        "deps": {
            "components": [
            ],
            "third_party": [
                "cJSON",
                "bounds_checking_function"
            ]
        },
        "build": {
            "sub_component": [
                "//base/startup/init_lite/interfaces/innerkits:libbegetutil",
                "//base/startup/init_lite/services:init_lite",
                "//base/startup/init_lite/services/begetctl:begetctl_cmd",
                "//base/startup/init_lite/watchdog:watchdog",
                "//base/startup/init_lite/ueventd:ueventd"
            ],
            "inner_kits": [
                {
                    "header": {
                        "header_base": "//base/startup/init_lite/interfaces/innerkits/include/",
                        "header_files": [
                            "beget_ext.h",
                            "syspara/parameter.h",
                            "syspara/parameters.h",
                            "syspara/param_wrapper.h",
                            "syspara/sysparam_errno.h",
                            "syspara/sysversion.h"
                        ]
                    },
                    "name": "//base/startup/init_lite/interfaces/innerkits:libbegetutil"
                }
            ],
            "test": [
                "//base/startup/init_lite/test/unittest/lite:init_test"
            ]
        }
    }
}